extends layout

block content
	h1= title
	.container
		ul
			each val in memoArray
				li(style='margin-bottom: 10px; font-size: 20px !important;')
					a(href='/memo/' + val['_id'])= '[' + val['author'] + ']' + val['memo']
		a.btn.btn-block.btn-primary(href='/memo/new' style='margin-top:30px;')                               
			| New Memo 
